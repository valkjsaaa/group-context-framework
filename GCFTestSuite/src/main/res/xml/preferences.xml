<?xml version="1.0" encoding="utf-8"?>

<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    
        <PreferenceCategory 
        android:title="Application Details"
        android:key="category_app_details">
        
        <EditTextPreference 
            android:key="app_version"
            android:title="Application Version" 
            android:enabled="false"
            android:selectable="false"
            android:persistent="false"
            android:shouldDisableView="false"/>
        
        <EditTextPreference 
            android:key="gcf_version"
            android:title="GCF Version" 
            android:enabled="false"
            android:selectable="false"
            android:persistent="false"
            android:shouldDisableView="false"/>
        
        <EditTextPreference 
            android:key="gcf_comm"
            android:title="Comm Channel" 
            android:enabled="false"
            android:selectable="false"
            android:persistent="false"
            android:shouldDisableView="false"/>
        
        <EditTextPreference 
            android:key="device_name"
            android:title="GCF Name"
            android:summary="Set Blank to Use Programmatic Name" />
        
    </PreferenceCategory>
